<?xml version="1.0" encoding="utf-8"?>
<!--状态选择器-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--当元素未被按压时触发-->
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="74dp"/>
            <solid android:color="#FFFFFF"/>
        </shape>
    </item>


</selector>